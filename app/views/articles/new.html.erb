<div id='new_articles'>
	<form action='/articles' method='POST'>
	  <h2>Please add a new Article</h2>
	  <%= token_tag nil %>
	  <input type="text" class="input-block-level" placeholder="Article Title" name = "article_title"></br>
	  <input type="text" class="input-block-level" placeholder="Article Body" name = "article_body"></br>
	  <button class="btn btn-large btn-primary" type="submit">Submit</button>
	</form>
</div>
