<h2>List of Items:</h2>

<% @articles.each do |article| %>
<table border="1" width="800">
	<tr>
		<td width="300"><a href="/articles/<%= article.id %>"><%= article.title %></h2></a></td>
		<td width="300"><p><%= article.body %></p></td>
		<td width="200"><a href="/articles/<%= article.id %>/edit" >edit</a></td>
	</tr>

<% end %>
